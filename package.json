{
  "name": "fuel-state-manager",
  "version": "1.0.2",
  "description": "Manages custom pushState, replaceState, and popstates.",
  "keywords": [
    "pushstate",
    "navigation",
    "popstate"
  ],
  "main": "manager.js",
  "types": "./src/global.d.ts",
  "files": [
    "src",
    "lib",
    "manager.js",
    "manager.js.map"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "scripts": {
    "precommit": "tslint -c .tslintrc.js -p tsconfig.json -t verbose",
    "compile": "tsc",
    "bundle": "webpack",
    "demo": "http-server ./docs -o",
    "test": "npm run compile && npm run bundle && npm run demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pageworks/fuel-state-manager.git"
  },
  "author": "Kyle Andrews",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pageworks/fuel-state-manager/issues"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
